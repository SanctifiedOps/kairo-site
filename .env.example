# Telegram Bot Configuration
# Get bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=

# Your Telegram channel ID (format: @your_channel or -100XXXXXXXXXX)
TELEGRAM_CHANNEL_ID=

# Path to video file to attach with each transmission
TELEGRAM_VIDEO_PATH=./public/assets/kairo-bg.mp4

# Optional: public URL for video (useful on Netlify)
TELEGRAM_VIDEO_URL=

# Enable/disable Telegram posting (true/false)
TELEGRAM_POSTING_ENABLED=true

# One-time reset token to restart cycle index at 0
# Change the value to trigger a new reset
CYCLE_RESET_VERSION=

# Other existing environment variables
PORT=8787
ADMIN_KEY=
MODEL_PRIMARY=
MODEL_SECONDARY=
CLAUDE_MODEL=claude-3-5-sonnet-latest
OPENAI_API_KEY=
OPENAI_AUDITOR_MODEL=
SOLANA_RPC_URL=
TOKEN_MINT_ADDRESS=
TOKEN_MIN_BALANCE=100000
ENABLE_TOKEN_GATING=false
